<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Remove Expense'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Remove Expense'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'SyncExpenseTest.it_can_sync_expense'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'SyncExpenseTest.it_can_sync_expense'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Remove Expense'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Remove Expense'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Tests'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Tests'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'SyncExpenseTest.it_can_sync_expense'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'SyncExpenseTest.it_can_sync_expense'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>